{
   "name": "clean-arquitecture-template",
   "version": "1.0.0",
   "description": "template for init projects with clean arquitecture",
   "main": "index.js",
   "scripts": {
      "dev": "npx nodemon ./src/index.ts",
      "dev:ddd": "nodemon ./source/user/user.app.ts",
      "build": "npx tsc -p tsconfig.json",
      "generate": "plop --plopfile ./generators/index.js",
      "migrate": "npx prisma migrate dev init",
      "migration-pull-print": "npx prisma db pull --print",
      "migration:push": "npx prisma db push",
      "migration:push:test": "npx env-cmd --file .env.test prisma db push",
      "db:create": "pnpm migration:push && pnpm migration:push:test",
      "prisma-generate": "npx prisma generate",
      "typeorm": "typeorm-ts-node-commonjs",
      "test": "jest",
      "test-coverage": "jest --coverage",
      "test-watch": "jest --watch",
      "lint:fix": "eslint ./src --ext .jsx,.js,.ts,.tsx --quiet --fix",
      "prettier": "prettier --write \"./**/*.{js,jsx,ts,tsx,css,md,json}\" --config ./.prettierrc",
      "lint": "pnpm prettier && pnpm lint:fix",
      "type-check": "tsc",
      "prepare": "husky install",
      "start": "ts-node src/index.ts"
   },
   "author": "juanfer",
   "license": "ISC",
   "dependencies": {
      "@prisma/client": "^4.1.1",
      "body-parser": "^1.20.0",
      "class-validator": "^0.13.2",
      "cors": "^2.8.5",
      "dotenv": "^16.0.1",
      "env-cmd": "^10.1.0",
      "express": "^4.18.1",
      "express-requests-logger": "^3.0.4",
      "express-serve-static-core": "^0.1.1",
      "inversify": "^6.0.1",
      "inversify-express-utils": "^6.4.3",
      "logform": "^2.4.2",
      "method-override": "^3.0.0",
      "mongoose": "^6.5.1",
      "morgan": "^1.10.0",
      "reflect-metadata": "^0.1.13",
      "typeorm": "0.3.10",
      "uuid": "^8.3.2",
      "winston": "^3.8.1",
      "pg": "^8.4.0"
   },
   "devDependencies": {
      "@babel/plugin-proposal-class-properties": "^7.18.6",
      "@babel/plugin-proposal-decorators": "^7.18.10",
      "@babel/preset-env": "^7.18.10",
      "@babel/preset-typescript": "^7.18.6",
      "@faker-js/faker": "^7.5.0",
      "@types/body-parser": "^1.19.2",
      "@types/cors": "^2.8.12",
      "@types/dotenv": "^8.2.0",
      "@types/express": "^4.17.13",
      "@types/inversify": "^2.0.33",
      "@types/jest": "^28.1.6",
      "@types/method-override": "^0.0.32",
      "@types/mongoose": "^5.11.97",
      "@types/morgan": "^1.9.3",
      "@types/nodemon": "^1.19.2",
      "@types/supertest": "^2.0.12",
      "@types/uuid": "^8.3.4",
      "@typescript-eslint/eslint-plugin": "^5.39.0",
      "@typescript-eslint/parser": "^5.39.0",
      "babel-plugin-parameter-decorator": "^1.0.16",
      "babel-plugin-transform-typescript-metadata": "^0.3.2",
      "eslint": "^8.24.0",
      "jest": "^28.1.3",
      "jest-mock-extended": "2.0.4",
      "nodemon": "^2.0.19",
      "prettier": "^2.7.1",
      "prisma": "^4.1.1",
      "supertest": "^6.2.4",
      "ts-jest": "^28.0.8",
      "ts-node": "10.7.0",
      "typescript": "4.5.2",
      "@types/node": "^16.11.10"
   }
}